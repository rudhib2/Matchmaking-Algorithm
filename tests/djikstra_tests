// attribute - 3rd = attr_attrib
// A~0 compatible with 1-15 (not 0 because it cant be compatible to itself)

// 6, 14, 42?, 51?, 535?

// 4+6+6+8 = 24 
//let's keep threshold 25 


TEST_CASE("BFS Test 1 ~ Compatible", "[weight=1]") {
    
        REQUIRE(pref_score[4] = 370);
        REQUIRE(attr_score[14] = 330);
        REQUIRE(isCompatible(4, 14) = true);
}

TEST_CASE("BFS Test 1 ~ Compatible-basic", "[weight=1]") {
    
        REQUIRE(pref_score[4] = 370);
        REQUIRE(attr_score[14] = 330);
        REQUIRE(isCompatible(4, 14) = true);
}
